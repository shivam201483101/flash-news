{
  "version": 2,
  "builds": [
    {
      "src": "newsapp/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "newsapp/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "PIP_NO_WARN_SCRIPT_LOCATION": "0"
    }
  }
}
